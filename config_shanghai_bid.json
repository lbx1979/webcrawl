      }
    },
    "JZMJ": {
      "default": ""
    },
    "ZBLX": {
      "name": "span",
      "attrs": {
        "id": "ctl00_ContentPlaceHolder1_InfoGcmsSg_lblgclb"
      }
    },
    "ZBFW": {
      "default": ""
    },
    "HTGSJ": {
      "name": "span",
        "attrs": {
          "id": "ctl00_ContentPlaceHolder1_InfoGctz1_lblgcgys_net"
      }
